[user] # Place your user info here
  name = Henlium
  email = henliumlee@gmail.com
  username = henlium
[alias]
  a = add
  aa = add --all
  ##########
	b = branch
  ##########
  c = commit
  cm = commit -m
  ##########
	co = checkout
  ##########
  cl = clone
  ##########
  d = diff
  ##########
	l = log --oneline --graph
	ls = log --oneline --graph --decorate
  ##########
	s = status
  sh = stash
  ##########
  aliases = !git config -l | grep alias | cut -c 7-
[core]
	editor = vim
	excludesfile = /Users/user/.gitignore_global
[commit]
  template = ~/.gitmessage
